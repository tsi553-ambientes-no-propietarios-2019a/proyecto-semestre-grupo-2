{% extends 'base.html.twig' %}

{% block title %}Places{% endblock %}

{% block body %}
    <h1>Places</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ place.id }}</td>
            </tr>
            <tr>
                <th>Id_lugar</th>
                <td>{{ place.idLugar }}</td>
            </tr>
            <tr>
                <th>Cod_city</th>
                <td>{{ place.codCity }}</td>
            </tr>
            <tr>
                <th>Id_pakage</th>
                <td>{{ place.idPakage }}</td>
            </tr>
            <tr>
                <th>Name_place</th>
                <td>{{ place.namePlace }}</td>
            </tr>
            <tr>
                <th>Longitud</th>
                <td>{{ place.longitud }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
                <td>{{ place.latitude }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('places_index') }}">back to list</a>

    <a href="{{ path('places_edit', {'id': place.id}) }}">edit</a>

    {{ include('places/_delete_form.html.twig') }}
