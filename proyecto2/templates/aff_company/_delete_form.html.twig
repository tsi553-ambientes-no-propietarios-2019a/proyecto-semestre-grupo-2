<form method="post" action="{{ path('aff_company_delete', {'id': aff_company.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ aff_company.id) }}">
    <button class="btn">Delete</button>
</form>
