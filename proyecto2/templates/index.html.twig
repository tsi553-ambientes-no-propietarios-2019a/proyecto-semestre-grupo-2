{% block section %}

<!-- start banner Area -->
			<section class="banner-area relative">
				<div class="overlay overlay-bg"></div>
				<div class="container">
					<div class="row fullscreen align-items-center justify-content-between">
						<div class="col-lg-6 col-md-6 banner-left">
							<h1 class="text-white">Colecciona Momentos</h1>
							<h6 class="text-white">
								Sueña sin miedo, vive sin límites, viaja sin fronteras
							</h6>
              </br>
              {% if is_granted("IS_AUTHENTICATED_REMEMBERED") == false %}
							<a href="http://localhost:8000/register/" class="primary-btn text-uppercase">Registrarse</a>
              {% endif %}
						</div>
						<div class="col-lg-4 col-md-6 banner-right">
							<ul class="nav nav-tabs" id="myTab" role="tablist">

							  <li class="nav-item">
							    <a class="nav-link active" id="flight-tab" data-toggle="tab" href="#flight" role="tab" aria-controls="salidas" aria-selected="true">Salidas</a>
							  </li>

							  <li class="nav-item">
							    <a class="nav-link" id="hotel-tab" data-toggle="tab" href="#hotel" role="tab" aria-controls="hoteles" aria-selected="false">Hoteles</a>
							  </li>

							  <li class="nav-item">
							    <a class="nav-link" id="holiday-tab" data-toggle="tab" href="#holiday" role="tab" aria-controls="paquetes" aria-selected="false">Paquetes</a>
							  </li>

							</ul>
							<div class="tab-content" id="myTabContent">

							  <div class="tab-pane fade show active" id="flight" role="tabpanel" aria-labelledby="salidas-tab">
								<form class="form-wrap">
									<input type="text" class="form-control" name="name" placeholder="Origen" onfocus="this.placeholder = ''" onblur="this.placeholder = 'From '">
									<input type="text" class="form-control" name="to" placeholder="Destino" onfocus="this.placeholder = ''" onblur="this.placeholder = 'To '">
									<input type="text" class="form-control date-picker" name="start" placeholder="Salida" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Start '">
									<input type="text" class="form-control date-picker" name="return" placeholder="Retorno" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Return '">
									<input type="number" min="1" max="20" class="form-control" name="adults" placeholder="Adultos" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Adults '">
									<input type="number" min="1" max="20" class="form-control" name="child" placeholder="Niños" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Child '">
									<a href="http://localhost:8000/compra?aux=sadad" class="primary-btn text-uppercase">Buscar Salidas</a>
								</form>
							  </div>

							  <div class="tab-pane fade" id="hotel" role="tabpanel" aria-labelledby="hoteles-tab">
								<form class="form-wrap">
									<input type="text" class="form-control" name="name" placeholder="Destino" onfocus="this.placeholder = ''" onblur="this.placeholder = 'From '">
									<input type="text" class="form-control date-picker" name="start" placeholder="Check-In" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Start '">
									<input type="text" class="form-control date-picker" name="return" placeholder="Check-out" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Return '">
									<input type="number" min="1" max="20" class="form-control" name="adults" placeholder="Adultos " onfocus="this.placeholder = ''" onblur="this.placeholder = 'Adults '">
									<input type="number" min="1" max="20" class="form-control" name="child" placeholder="Niños" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Child '">
									<a href="#" class="primary-btn text-uppercase">Buscar Hoteles</a>
								</form>
							  </div>

							  <div class="tab-pane fade" id="holiday" role="tabpanel" aria-labelledby="paquetes-tab">
								<form class="form-wrap">
									<input type="text" class="form-control" name="name" placeholder="From " onfocus="this.placeholder = ''" onblur="this.placeholder = 'From '">
									<input type="text" class="form-control" name="to" placeholder="To " onfocus="this.placeholder = ''" onblur="this.placeholder = 'To '">
									<input type="text" class="form-control date-picker" name="start" placeholder="Start " onfocus="this.placeholder = ''" onblur="this.placeholder = 'Start '">
									<input type="text" class="form-control date-picker" name="return" placeholder="Return " onfocus="this.placeholder = ''" onblur="this.placeholder = 'Return '">
									<input type="number" min="1" max="20" class="form-control" name="adults" placeholder="Adults " onfocus="this.placeholder = ''" onblur="this.placeholder = 'Adults '">
									<input type="number" min="1" max="20" class="form-control" name="child" placeholder="Child " onfocus="this.placeholder = ''" onblur="this.placeholder = 'Child '">
									<a href="#" class="primary-btn text-uppercase">Buscar Paquetes</a>
								</form>
							  </div>

							</div>
						</div>
					</div>
				</div>
			</section>
<!-- End banner Area -->

